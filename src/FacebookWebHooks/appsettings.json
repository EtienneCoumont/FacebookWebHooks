{
  "FacebookOptions": {
    // When configuring WebHooks in your Facebook App, you are asked to enter a Verify Token.
    // Set the same value here (any random string)
    "VerifyToken": "***",
    // When calling WebHooks, Facebook adds a SHA1 signature to the HTTP requests. 
    // You can verify it to ensure the authenticity of the message.
    // Disable the verification if you have to test some custom requests. Enable it in production.
    "ShouldVerifySignature": false,
    // Your App Secret (get it from https://developers.facebook.com/apps/ )
    "AppSecret": "***",
    // Your App Token (get it from https://developers.facebook.com/tools/accesstoken/ )
    "AppToken": "***"

  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
